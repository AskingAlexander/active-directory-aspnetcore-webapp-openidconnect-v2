{
    "AzureAdB2C": {
        "Instance": "https://<b2c-tenant-name>.b2clogin.com",
        "ClientId": "<b2c-webApp-clientId>",
        "Domain": "<your-b2c-tenant>", //for example fabrikamb2c.onmicrosoft.com
        "ClientSecret": "<b2c-webApp-secret>",
        "SignedOutCallbackPath": "/signout/<your-signUp-signIn-policy>",
        "SignUpSignInPolicyId": "<your-signUp-signIn-policy>",
        "EditProfilePolicyId": "<your-edit-profile-policy>" // Optional profile editing policy
        //"CallbackPath": "/signin/B2C_1_sign_up_in"  // defaults to /signin-oidc
    },
    "TodoList": {
        "TodoListScope": "https://<your-b2c-webApi-scope>",
        "TodoListBaseAddress": "https://localhost:44351"

    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AllowedHosts": "*"
}
